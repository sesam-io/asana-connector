[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "namespaced_identifiers": false,
    "source": {
      "operation": "list",
      "properties": {
        "datatype": "{{@ datatype @}}"
      },
      "system": "{{@ system @}}",
      "type": "rest"
    },
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "namespaced_identifiers": false,
    "source": {
      "completeness": false,
      "dataset": "{{@ system @}}-{{@ datatype @}}-all",
      "if_source_empty": "accept",
      "type": "dataset"
    },
    "transform": [
      {
        "id_expression": "{{ gid }}",
        "operation": "lookup",
        "properties": {
          "datatype": "{{@ datatype @}}"
        },
        "replace_entity": false,
        "system": "{{@ system @}}",
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "merge",
              "_S.response"
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  }
]